-- Godmode
local godmode = false
local godBtn = Instance.new("TextButton", dialog)
godBtn.Position = UDim2.new(0,18,0,195)
godBtn.Size = UDim2.new(0,140,0,28)
godBtn.Text = "Godmode: OFF"
