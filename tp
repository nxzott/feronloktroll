-- Fitur utama
local function makeSwitch(text, color, posY)
    local btn = Instance.new("TextButton")
    btn.Parent = dialog
    btn.Position = UDim2.new(0,20,0,posY)
    btn.Size = UDim2.new(0,145,0,26)
    btn.Text = text .. ": OFF"
    btn.BackgroundColor3 = DARK
    btn.TextColor3 = color
    btn.Font = Enum.Font.GothamBold
    btn.TextSize = 13
    local cr = Instance.new("UICorner", btn)
    cr.CornerRadius = UDim.new(0,8)
    return btn
end

local function makeAction(text, color, posY)
    local btn = Instance.new("TextButton")
    btn.Parent = dialog
    btn.Position = UDim2.new(0,190,0,posY)
    btn.Size = UDim2.new(0,145,0,26)
    btn.Text = text
    btn.BackgroundColor3 = ACCENT2
    btn.TextColor3 = color
    btn.Font = Enum.Font.GothamBold
    btn.TextSize = 13
    local cr = Instance.new("UICorner", btn)
    cr.CornerRadius = UDim.new(0,8)
    return btn
end

