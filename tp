    god = not god
    godBtn.Text = "GodMode: " .. (god and "ON" or "OFF")
    godBtn.TextColor3 = god and ACCENT or Color3.fromRGB(255,80,200)
end)

local flingBtn = makeAction("Fling Self", WHITE, 154)
flingBtn.MouseButton1Click:Connect(function()
    local hrp = lp.Character and lp.Character:FindFirstChild("HumanoidRootPart")
    if hrp then
        hrp.Velocity = Vector3.new(math.random(-300,300),300,math.random(-300,300))
    end
end)

