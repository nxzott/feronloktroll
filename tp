soundBtn.MouseButton1Click:Connect(function()
    local s = Instance.new("Sound", workspace)
    s.SoundId = "rbxassetid://138186576" -- bell
    s.Volume = 5
    s:Play()
    game.Debris:AddItem(s,2)
end)

