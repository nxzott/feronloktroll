end)

-- Glitch Camera
local camBtn = makeAction("Glitch Cam", WHITE, 454)
camBtn.MouseButton1Click:Connect(function()
    local cam = workspace.CurrentCamera
    for i=1,32 do
        cam.CFrame = cam.CFrame * CFrame.Angles(0,math.rad(math.random(-2,2)),0)
        task.wait(0.03)
    end
end)

