    freeze = not freeze
    freezeBtn.Text = "Freeze: " .. (freeze and "ON" or "OFF")
    local hrp = lp.Character and lp.Character:FindFirstChild("HumanoidRootPart")
    if hrp then
        if freeze then
            hrp.Anchored = true
        else
            hrp.Anchored = false
        end
    end
end)

